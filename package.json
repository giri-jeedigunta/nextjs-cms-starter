{
  "name": "next-with-authored-pages",
  "version": "1.0.0",
  "description": "Next.js with authored layouts",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run mock-server\" \"nodemon server\"",
    "build": "next build",
    "export": "next export",
    "build:serve": "npm run build && npm run start",
    "start": "concurrently \"npm run mock-server\" \"NODE_ENV=production node server\"",
    "mock-server": "canned -p 5000 --relaxed_accept=true ./src/mocks/"
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-typescript": "^1.1.1",
    "es6-promise": "^4.2.5",
    "exenv": "^1.2.2",
    "express-http2-workaround": "^1.1.3",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "next": "latest",
    "next-routes": "^1.4.2",
    "next-runtime-dotenv": "^1.0.1",
    "react": "^16.0.0",
    "react-axe": "^3.1.0",
    "react-dom": "^16.0.0",
    "react-styled-flexboxgrid": "^3.1.0",
    "styled-components": "^4.2.0",
    "types-installer": "^1.6.3"
  },
  "author": "Rakesh Menon",
  "license": "ISC",
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/exenv": "^1.2.0",
