    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/next": "^8.0.4",
    "@types/react": "^16.8.15",
    "@types/react-axe": "^3.1.0",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.14",
    "@types/zeit__next-typescript": "^0.1.2",
    "canned": "^0.3.12",
    "concurrently": "^4.0.1",
    "next-awesome-typescript": "^0.3.2",
    "nodemon": "^1.18.4"
  }
}
